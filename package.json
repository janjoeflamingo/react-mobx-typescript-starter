{
  "name": "react-mobx-typescript-starter",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear:cache": "rimraf -rf ./.cache/*",
    "prestart": "cross-env NODE_ENV=development",
    "prebuild": "cross-env NODE_ENV=production yarn lint",
    "postbuild": "yarn size",
    "postrelease": "react-snap",
    "start": "webpack-dev-server --config configs/run --env development",
    "build": "env NODE_ENV=production webpack --config configs/run --env production",
    "build:display": "env NODE_ENV=production webpack --display-modules --config configs/run --env production",
    "preanalyze": "yarn clear:cache",
    "analyze": "yarn build --analyze",
    "release": "yarn build --release",
    "profile": "webpack --config configs/run --env production --profile --json > stats.json",
    "lint": "tslint -p tsconfig.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc",
    "size": "size-limit",
    "pretest": "cross-env NODE_ENV=test yarn lint",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "ci": "yarn test & yarn typecheck & yarn lint",
    "lh": "lighthousebot -- --perf=96 --pwa=100",
    "c": "rimraf -rf ./dist/test && node ./component.js",
    "a": "yarn clear:cache && yarn analyze"
  },
  "reactSnap": {
    "source": "dist",
    "inlineCss": false,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": true
    }
  },
  "size-limit": [
    {
      "path": [
        "dist/static/*.css",
        "dist/static/*.js"
      ],
      "limit": "300 KB",
      "webpack": false,
      "gzip": true
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn ci"
    }
  },
  "keywords": [
    "react"
  ],
  "author": "Edik Bulikyan <ed.proff@gmail.com>",
  "license": "MIT",
  "devDependencies": {
