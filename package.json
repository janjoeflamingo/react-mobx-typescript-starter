{
  "name": "react-mobx-typescript-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "cross-env NODE_ENV=development ",
    "prebuild": "cross-env NODE_ENV=production yarn lint",
    "postbuild": "yarn size",
    "start": "webpack-dev-server --config configs/run --env development",
    "build": "env NODE_ENV=production webpack --config configs/run --env production",
    "build:display": "env NODE_ENV=production webpack --display-modules --config configs/run --env production",
    "profile": "webpack --config configs/run --env production --profile --json > stats.json",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc",
    "size": "size-limit",
    "prettier:check": "prettier -- --list-different 'src/**/*.{ts,tsx}'",
    "prettier:write": "prettier -- --write 'src/**/*.{ts,tsx}'",
    "test": "jest --env=jsdom",
    "test:watch": "yarn test -- --watch"
  },
  "size-limit": [
    {
      "path": [
        "dist/static/*.css",
        "dist/static/*.js"
      ],
      "limit": "300 KB",
      "webpack": false,
      "gzip": true
    }
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/node": "10.12.24",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-router-dom": "4.3.1",
    "@types/styled-components": "4.1.8",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "1.0.1",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prettier": "1.16.4",
    "react-hot-loader": "4.6.5",
    "size-limit": "0.21.1",
    "source-map-loader": "0.2.4",
    "terser-webpack-plugin": "1.2.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3",
    "url-loader": "1.1.2",
    "webpack": "4.29.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-chunk-hash": "0.6.0",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-loading-skeleton": "1.0.0",
    "react-router-dom": "4.3.1",
    "storage.io": "1.2.0",
    "styled-components": "4.1.3"
  }
}
