{
  "name": "react-mobx-typescript-starter",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "cross-env NODE_ENV=development ",
    "prebuild": "cross-env NODE_ENV=production yarn lint",
    "postbuild": "yarn size",
    "start": "webpack-dev-server --config configs/run --env development",
    "build": "env NODE_ENV=production webpack --config configs/run --env production",
    "build:display": "env NODE_ENV=production webpack --display-modules --config configs/run --env production",
    "analyze": "yarn build --analyze",
    "profile": "webpack --config configs/run --env production --profile --json > stats.json",
    "lint": "tslint -p tsconfig.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc",
    "size": "size-limit",
    "pretest": "cross-env NODE_ENV=test yarn lint",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "ci2": "yarn test & yarn typecheck & yarn lint",
    "ci": "yarn test & yarn lint",
    "lh": "lighthousebot -- --perf=96 --pwa=100",
    "b": "rm -rf ./dist/test && node ./component.js",
    "a": "rm -rf ./.cache && yarn analyze"
  },
  "size-limit": [
    {
      "path": [
        "dist/static/*.css",
        "dist/static/*.js"
      ],
      "limit": "300 KB",
      "webpack": false,
      "gzip": true
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn ci"
    }
  },
  "keywords": [
    "react"
  ],
  "author": "Edik Bulikyan <ed.proff@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/enzyme": "3.1.18",
    "@types/enzyme-adapter-react-16": "1.0.4",
    "@types/jest": "24.0.6",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/react-router-dom": "4.3.1",
    "@types/react-test-renderer": "16.8.1",
    "@types/styled-components": "4.1.10",
    "@types/webpack-env": "1.13.7",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "cache-loader": "2.0.1",
    "clean-webpack-plugin": "1.0.1",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "enzyme-to-json": "3.3.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "image-webpack-loader": "4.6.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-webpack-plugin": "2.4.2",
    "jest": "24.1.0",
    "json-loader": "0.5.7",
    "lighthousebot": "https://github.com/GoogleChromeLabs/lighthousebot",
    "lint-staged": "8.1.4",
    "markdown-loader": "5.0.0",
    "marked": "0.6.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "raw-loader": "1.0.0",
    "react-hot-loader": "4.7.0",
    "size-limit": "0.21.1",
    "source-map-loader": "0.2.4",
    "svg-url-loader": "2.3.2",
    "terser-webpack-plugin": "1.2.2",
    "ts-jest": "24.0.0",
    "tslint": "5.12.1",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3",
    "url-loader": "1.1.2",
    "webpack": "4.29.5",
    "webpack-bundle-analyzer": "3.0.4",
    "webpack-chunk-hash": "0.6.0",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.9.1",
    "jest-styled-components": "6.3.1",
    "mobx": "5.9.0",
    "mobx-react": "5.4.3",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-loading-skeleton": "1.0.0",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.8.2",
